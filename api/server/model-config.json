{
  "_meta": {
    "sources": [
      "loopback/common/models",
      "loopback/server/models",
      "../common/models",
      "./models"
    ],
    "mixins": [
      "loopback/common/mixins",
      "loopback/server/mixins",
      "../common/mixins",
      "./mixins"
    ]
  },
  "User": {
    "dataSource": "mongodb",
    "public": false
  },
  "AccessToken": {
    "dataSource": "mongodb",
    "public": false
  },
  "ACL": {
    "dataSource": "mongodb",
    "public": false
  },
  "RoleMapping": {
    "dataSource": "mongodb",
    "public": false,
    "options": {
      "strictObjectIDCoercion": true
    }
  },
  "Role": {
    "dataSource": "mongodb",
    "public": false
  },
  "user": {
    "dataSource": "mongodb",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "*": false,
          "login": true,
          "logout": true,
          "create": true,
          "findById": true,
          "prototype.__create__lists": true,
          "prototype.__unlink__lists": true,
          "prototype.__destroyById__lists": true,
          "prototype.__updateById__lists": true
        }
      }
    }
  },
  "list": {
    "dataSource": "mongodb",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "*": false,
          "findById": true,
          "getPublicList": true,
          "prototype.patchAttributes": true,
          "prototype.__get__destinations": true,
          "prototype.__create__destinations": true,
          "prototype.__destroyById__destinations": true
        }
      }
    }
  },
  "destination": {
    "dataSource": "mongodb",
    "public": false
  },
  "invite": {
    "dataSource": "mongodb",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "*": false,
          "create": true,
          "findById": true,
          "answer": true
        }
      }
    }
  }
}
